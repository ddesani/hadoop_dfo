<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<!-- Put site-specific property overrides in this file. -->

<configuration>
<% @props.each do |p| %>
	<property>
		<name><%= p['name'] %></name>
		<value><%= p['value'] %></value>
		<description><%= p['description'] %></description>
	</property>
<% end %>
</configuration>